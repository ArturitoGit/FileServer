{
  "name": "file_server",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Arturito",
  "description": "Move easily your file between two of your devices !",
  "scripts": {
    "build": ".\\node_modules\\.bin\\tsc",
    "start": "electron-forge start",
